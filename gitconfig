[core]
	editor = vim
  excludesfile = ~/.gitignore
[color]
	diff = auto
	status = auto
	branch = auto
[alias]
  qc = commit --message \"No Message Logged\" --cleanup=default --all
  changes = diff --name-status -r
  diffstat = diff --stat -r
  allstatus = status --ignore-submodules=none
  find = ls-tree -r --full-name --name-status HEAD
[status]
  submodulesummary = true
[branch]
  autosetupmerge = true
[github]
	user = stick
  token = !security 2>&1 >/dev/null find-generic-password -gs github.token | ruby -e 'print $1 if STDIN.gets =~ /^password: \\\"(.*)\\\"$/'
